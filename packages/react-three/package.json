{
  "name": "@liqvid/react-three",
  "version": "1.0.2",
  "description": "@react-three integration for Liqvid",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "pnpm build:clean && pnpm build:cjs && pnpm build:esm && pnpm build:fix-esm",
    "build:clean": "rm -fr dist",
    "build:cjs": "tsc --module commonjs && mv dist/index.js dist/index.cjs",
    "build:esm": "tsc --module esnext && mv dist/index.js dist/index.mjs",
    "build:fix-esm": "sed --in-place --expression 's/useContextBridge\"/useContextBridge.js\"/g' --expression 's/jsx-runtime/jsx-runtime.js/g' dist/index.mjs",
    "lint": "eslint --ext ts,tsx --fix src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liqvidjs/liqvid.git"
  },
  "author": "Yuri Sulyma <yuri@liqvidjs.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liqvidjs/liqvid/issues"
  },
  "homepage": "https://github.com/liqvidjs/liqvid/tree/main/packages/react-three",
  "dependencies": {
    "@juggle/resize-observer": "^3.3.1",
    "@react-three/drei": "^8.19.4",
    "@types/events": "^3.0.0",
    "@types/react-reconciler": "^0.26.4"
  },
  "devDependencies": {
    "@react-three/fiber": "^7.0.26",
    "@types/three": "^0.139.0",
    "events": "^3.3.0",
    "liqvid": "workspace:^",
    "strict-event-emitter-types": "^2.0.0",
    "three": "^0.139.0"
  },
  "peerDependencies": {
    "@react-three/fiber": "^7.0.0",
    "liqvid": "workspace:^",
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "sideEffects": false,
  "type": "module"
}
