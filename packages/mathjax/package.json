{
  "name": "@liqvid/mathjax",
  "version": "0.0.1",
  "description": "MathJax integration for Liqvid",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./plain": {
      "import": "./dist/esm/plain.js",
      "require": "./dist/cjs/plain.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/types/*"
      ]
    }
  },
  "files": [
    "dist/*"
  ],
  "author": "Yuri Sulyma <yuri@liqvidjs.org>",
  "keywords": [
    "liqvid",
    "mathjax"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liqvidjs/liqvid.git"
  },
  "bugs": {
    "url": "https://github.com/liqvidjs/liqvid/issues"
  },
  "homepage": "https://github.com/liqvidjs/liqvid/tree/main/packages/mathjax",
  "license": "MIT",
  "peerDependencies": {
    "@types/react": ">=17.0.0",
    "liqvid": "workspace:^",
    "mathjax": "^3.2.0",
    "react": ">=17.0.0"
  },
  "peerDependenciesMeta": {
    "liqvid": {
      "optional": true
    },
    "mathjax": {
      "optional": true
    }
  },
  "devDependencies": {
    "liqvid": "workspace:^",
    "mathjax": "^3.2.0"
  },
  "dependencies": {
    "@liqvid/utils": "workspace:^"
  }
}
