{
  "name": "liqvid",
  "version": "2.1.0",
  "description": "Library for playing interactive videos using HTML/CSS/Javascript",
  "files": [
    "dist/*"
  ],
  "main": "./dist/liqvid.js",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/liqvid.js"
  },
  "typings": "./dist/types/index.d.ts",
  "scripts": {
    "build": "pnpm build:clean && pnpm build:css && pnpm build:cjs && pnpm build:esm && pnpm build:postclean",
    "build:clean": "rm -rf dist",
    "build:cjs": "webpack; NODE_ENV=production webpack",
    "build:css": "stylus -o dist/liqvid.css styl/liqvid.styl; stylus -c -o dist/liqvid.min.css styl/liqvid.styl",
    "build:esm": "pnpm build:esm:tsc && pnpm build:esm:fix",
    "build:esm:tsc": "tsc --module esnext --outDir dist/esm --target es2021",
    "build:esm:fix": "node ./build.mjs",
    "build:postclean": "find . -name tsconfig.tsbuildinfo -delete",
    "lint": "eslint --ext ts,tsx --fix src; eslint --ext ts,tsx --fix tests",
    "stylus": "stylus -o dist/liqvid.css -w styl/liqvid.styl",
    "stylus:prod": "stylus -c -o dist/liqvid.min.css -w styl/liqvid.styl",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liqvidjs/player.git"
  },
  "author": "Yuri Sulyma <yuri@liqvidjs.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liqvidjs/player/issues"
  },
  "homepage": "https://github.com/liqvidjs/player#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^14.0.0-beta.13",
    "@types/jest": "^27.4.1",
    "@yuri/eslint-config": "^1.0.1",
    "banner-webpack-plugin": "^0.2.3",
    "eslint": "^8.11.0",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.5.1",
    "nib": "^1.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.70.0",
    "stylus": "^0.56.0",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.8",
    "typedoc": "^0.22.13",
    "typedoc-plugin-markdown": "^3.11.14",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@liqvid/keymap": "1.1.2",
    "@liqvid/playback": "1.1.3",
    "@liqvid/utils": "^1.3.0",
    "@types/events": "^3.0.0",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "events": "^3.3.0",
    "react": "^17.0.2",
    "strict-event-emitter-types": "^2.0.0"
  }
}
